{
  "connectionString": "${file:/mnt/secrets/lau/app-insights-connection-string}",
  "role": {
    "name": "lau-case-backend"
  },
  "sampling": {
    "overrides": [
      {
        "telemetryType": "request",
        "attributes": [
          {
            "key": "http.url",
            "value": "https?://[^/]+/health.*",
            "matchType": "regexp"
          }
        ],
        "percentage": 1
      },
      {
        "telemetryType": "dependency",
        "percentage": 0
      }
    ]
  }
}
